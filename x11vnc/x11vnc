service x11vnc-5899
{
        disable = no
	type			= UNLISTED
        port                    = 5899
	bind			= 127.0.0.1
        socket_type             = stream
        protocol                = tcp
        wait                    = no
        user                    = root
        passenv                 = PATH
        server			= /etc/x11vnc/x11vnc-xinetd.sh
	server_args		= root localcosadmin nmr
}
